{
  "name": "flashcards",
  "private": true,
  "description": "Flashcards with spaced repetition",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "test": "jest --config ./config/jest/config.js --detectOpenHandles --color",
    "test:watch": "npm run test -- --watchAll",
    "test:coverage": "npm run test -- --coverage",
    "typecheck": "tsc",
    "typecheck:watch": "npm run typecheck -- --watch",
    "lint:watch": "onchange --exclude-path ./config/ignorePatterns './**/*.{js,jsx,ts,tsx}' --",
    "lint:script": "eslint --ignore-path ./config/ignorePatterns --config ./config/eslint/config.prod.js --fix",
    "lint:scripts": "npm run lint:script -- './**/*.{js,jsx,ts,tsx}'",
    "lint:scripts:watch": "npm run lint:watch -- npm run lint:scripts",
    "lint:script:dev": "npm run lint:script -- --config ./config/eslint/config.dev.js",
    "lint:scripts:dev": "npm run lint:script:dev -- './**/*.{js,jsx,ts,tsx}'",
    "lint:scripts:dev:watch": "npm run lint:watch -- npm run lint:scripts:dev",
    "lint:style": "stylelint --ignore-path ./config/ignorePatterns --config ./config/stylelint/config.prod.js --fix",
    "lint:styles": "npm run lint:style -- './**/*.{js,jsx,ts,tsx}'",
    "lint:styles:watch": "npm run lint:watch -- npm run lint:styles",
    "lint:style:dev": "npm run lint:style --config ./config/stylelint/config.dev.js",
    "lint:styles:dev": "npm run lint:style:dev -- './**/*.{js,jsx,ts,tsx}'",
    "lint:styles:dev:watch": "npm run lint:watch -- npm run lint:styles:dev",
    "check:all": "npm run typecheck && npm run lint:styles && npm run lint:scripts && npm run test:coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint --config ./config/commitlint.config.js -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run check:all"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:script",
      "npm run lint:style"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "ignore",
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "consistent",
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jetthusher/flashcards"
  },
  "author": "Jett Husher <jetthusher@pm.me> (https://github.com/jetthusher/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jetthusher/flashcards/issues"
  },
  "homepage": "https://github.com/jetthusher/flashcards#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-do-expressions": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.3",
    "@semantic-release/release-notes-generator": "^9.0.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.2",
    "@types/styled-components": "^4.4.2",
    "@types/styled-theming": "^2.2.2",
    "@types/testing-library__jest-dom": "^5.0.1",
    "@types/testing-library__react": "^9.1.2",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "cross-env": "^7.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest-dom": "^2.0.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.0.1",
    "eslint-watch": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-cli": "^25.1.0",
    "lint-staged": "^10.0.7",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "semantic-release": "^17.0.2",
    "stylelint": "^13.1.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "connected-react-router": "^6.7.0",
    "core-js": "^3.6.4",
    "deep-freeze": "0.0.1",
    "history": "^4.10.1",
    "localforage": "^1.7.3",
    "query-string": "^6.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.0.1",
    "styled-theming": "^2.2.0",
    "typesafe-actions": "^5.1.0",
    "utility-types": "^3.10.0"
  }
}
